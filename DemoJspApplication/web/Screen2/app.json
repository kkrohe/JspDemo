{
    "name": "Screen2",

    "classpath": "${app.dir}/app",

    "framework": "ext",

    "theme": "ext-theme-crisp",

    "requires": [
    ],

    "sass": {
        "namespace": "Screen2"
    },

    "js": [
        {
            "path": "app.js",
            "bundle": true
        }
    ],

    "css": [
        {
            "path": "bootstrap.css",
            "bootstrap": true
        }
    ],

    "appCache": {
        "cache": [
            "index.html"
        ],
        "network": [
            "*"
        ],
        "fallback": []
    },

    "resources": [
    ],

    "ignore": [
        "^\\.svn$",
        "^\\.git$"
    ],

    "archivePath": "archive",

    "slicer": {
        "js": [
            {
                "path": "${app.dir}/sass/example/custom.js",
                "isWidgetManifest": true
            }
        ]
    },
    
    "production": {
    },
    
    "testing": {
    },
    
    "development": {
    },
    
    "bootstrap": {
        "base": "${app.dir}",
        "manifest": "bootstrap.json",
        "microloader": "microloader.js",
        "css": "bootstrap.css",
        "page": "../Screen2.jsp"
    },
    
    "output": {
        "base": "${workspace.build.dir}/${build.environment}/${app.name}",
        "page": {
            "path": "../Screen2.jsp",
            "enable": false
        },
        "manifest": {
            "path": "app.json",
            "embed": false
        },
        "microloader": {
            "path": "microloader.js",
            "embed": false
        },
        "cache": {
            "enable": false
        }
    },
    
    "id": "b1f486ed-bae9-4009-9799-f1e4897fea42"
}